{% extends '_base.tmpl' %}
{% load static %}
{% block title %}Projects application{% endblock %}

{% block body_contents %}
    <div class="container theme-showcase" role="main">
        {% include "_logo.tmpl" %} <H1>List of calls</H1>

        <ul>
            {% for call in calls %}
            <li><a href="{% url 'proposal_new' %}?call={{ call.pk }}">{{ call.long_name }}</a></li>
            {% endfor %}
        </ul>

    </div>
{% endblock %}
